@{
    ViewData["Title"] = "Data PNS";
    ViewData["PageName"] = "data_pns";
    //ViewData["Category1"] = "Pegawai";
    ViewBag.Parent = "Pegawai";
    ViewBag.Category = "Pegawai";
    ViewBag.Title = "Data PNS";
}

@section styles {
    <link rel="stylesheet" href="~/css/datagrid/datatables/datatables.bundle.css" />
    <link rel="stylesheet" href="~/css/addon.css" />
}

<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr" role="heading">
                <h2>Tabel Data PNS</h2>
            </div>
            <div class="panel-container show">
                <div id="listContent" class="panel-content">
                    <table id="tblData" class="table table-striped table-bordered" data-page-length="5">
                        <thead>
                            <tr>
                                <th>Nama</th>
                                <th>NIK</th>
                                <th>NIP</th>
                                <th>NRK</th>
                                <th>No HP</th>
                                <th>Aksi</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts {
    <script src="~/js/datagrid/datatables/datatables.bundle.js"></script>
    <script src="~/pages/main/pns.js"></script>    
    <script>
        $('#js-page-content').smartPanel();
    </script>
}

